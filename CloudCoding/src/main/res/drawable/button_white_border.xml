<?xml version="1.0" encoding="utf-8"?>

<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true">
        <shape>
            <solid android:color="@color/com_cloudcoding_white" />
            <stroke android:width="1dp" android:color="@color/com_cloudcoding_white"/>
        </shape>
    </item>


    <item >
        <shape>
            <solid android:color="@color/com_cloudcoding_clear" />
            <stroke android:width="1dp" android:color="@color/com_cloudcoding_white"/>
        </shape>
    </item>


</selector>